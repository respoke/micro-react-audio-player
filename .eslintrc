{
  "parser": "babel-eslint",
  "root": true,
  "rules": {
    "consistent-return": [0],
    "id-length": [0],
    "no-param-reassign": [0],
    "no-shadow": [0],
    "spaced-comment": [0],
    "comma-dangle": [0],
    "func-names": [0],
    "padded-blocks": [0],
    "no-use-before-define": [0],
    "no-case-declarations": [0]
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb/base",
  "plugins": [
      "react"
  ]
}
